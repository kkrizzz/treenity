{
  "name": "feathers",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "client:build": "NODE_ENV=production parcel build --target browser src/client/index.html --dist-dir dist/client-prod",
    "client:serve": "parcel serve --port 3101 src/client/index.html --dist-dir dist/client",
    "client:watch": "parcel watch src/client/index.html --dist-dir dist/client",
    "sol:build": "NODE_ENV=production parcel build --target browser src/mods/solarea/index.html --dist-dir dist/solarea-prod",
    "sol:serve": "parcel serve --port 3102 src/mods/solarea/index.html --dist-dir dist/solarea",
    "sol:watch": "parcel watch src/mods/solarea/index.html --dist-dir dist/solarea",
    "sol:deploy": "yarn clean && ./scripts/deploy-sol.sh",
    "sol:test": " ts-node src/mods/solarea/program-api/main.spec.ts",
    "sol:analyze": "yarn sol:build --reporter @parcel/reporter-bundle-analyzer",
    "server:build": "NODE_ENV=production parcel build --target node --dist-dir dist/server-prod src/server/index.ts",
    "server:demon": "DEBUG=@feathersjs/transport-commons/*;NODE_ENV=development;NODE_CONFIG_DIR=./config/solarea nodemon dist/server-dev/node",
    "server:watch": "parcel watch --target node --dist-dir dist/server-dev/ src/server/index.ts",
    "clean": "rm -rf .parcel-cache dist",
    "clean:prod": "rm -rf dist/*-prod",
    "jest": "jest --forceExit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lg": "parcel watch --target node --public-url ./src/logicEngine/test.ts",
    "postinstall": "patch-package"
  },
  "proxy": "http://localhost:3100",
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "alias": {
    "treenity": "./src/treenity",
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "engines": {
    "node": "14",
    "yarn": ">= 0.18.0"
  },
  "targets": {
    "browser": {
      "publicUrl": "/"
    },
    "node": {
      "context": "node",
      "engines": {
        "node": "14"
      }
    },
    "node-bundle": {
      "context": "node",
      "includeNodeModules": true,
      "engines": {
        "node": "14"
      }
    }
  },
  "sourceMap": true,
  "--types": "dist/",
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
