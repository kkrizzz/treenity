{
    "compilerOptions": {
      "target": "es6",
      "lib": ["es2020"],
      "module": "commonjs",
      "strict": true,
      "noImplicitAny": false,
      "outDir": "dist",
      "rootDir": "./src",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "useDefineForClassFields": true,
      "esModuleInterop": true,
      "resolveJsonModule": true
    },
    "exclude": [
        "test",
        "tree-old"
    ]
}
